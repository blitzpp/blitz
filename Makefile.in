install-demoDATA: $(demo_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(demodir)
	@list='$(demo_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d= ; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo " $(INSTALL_DATA) $$d$$p $(DESTDIR)$(demodir)/$$f"; \
	  $(INSTALL_DATA) $$d$$p $(DESTDIR)$(demodir)/$$f; \
	done

uninstall-demoDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(demo_DATA)'; for p in $$list; do \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo " rm -f $(DESTDIR)$(demodir)/$$f"; \
	  rm -f $(DESTDIR)$(demodir)/$$f; \
	done

